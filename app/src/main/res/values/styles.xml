<resources>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryDark">@color/primary_500</item>
        <item name="colorAccent">@color/primary</item>
    </style>

    <style name="AppTheme.noActionBar" parent="Theme.AppCompat.Light.NoActionBar">
    </style>

    <style name="TextView"></style>

    <style name="TextView.PoppinsNormal32spBlack">
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:textColor">#202020</item>
        <item name="android:textSize">32sp</item>
    </style>

    <style name="TextView.PoppinsNormal16spBlack">
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:textColor">#202020</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="EditText"></style>

    <style name="EditText.PoppinsNormal14spGrey">
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:hint">@color/hintColor</item>
        <item name="android:textSize">14sp</item>
    </style>

</resources>